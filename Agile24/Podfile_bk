platform :ios, '8.0'

target 'SwiftWeatherForecast' do
  # Comment this line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for SwiftWeatherForecast
  pod 'AFNetworking', '~> 2.6'
  pod "MagicalRecord"
  pod 'SwiftOpenWeatherMapAPI'
  use_frameworks!
  pod 'Firebase'

  target 'SwiftWeatherForecastTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'SwiftWeatherForecastUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
